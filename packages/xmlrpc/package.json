{
  "name": "@foxglove/xmlrpc",
  "version": "2.0.0",
  "description": "TypeScript library implementing an XMLRPC client and server with pluggable server backend",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "xmlrpc",
    "xml",
    "rpc",
    "http",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/foxglove/ros-typescript.git"
  },
  "author": {
    "name": "Foxglove",
    "email": "support@foxglove.dev"
  },
  "homepage": "https://github.com/foxglove/ros-typescript",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./nodejs": {
      "types": "./dist/nodejs/index.d.ts",
      "default": "./dist/nodejs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "eslint --report-unused-disable-directives --fix .",
    "lint:ci": "eslint --report-unused-disable-directives .",
    "prepack": "yarn build",
    "prepublishOnly": "yarn lint:ci && yarn test",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "dependencies": {
    "@foxglove/just-fetch": "^1.2.4",
    "byte-base64": "^1.1.0",
    "sax": "^1.2.4",
    "xmlbuilder2": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.8",
    "@types/sax": "^1.2.3",
    "jest": "^30.0.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
